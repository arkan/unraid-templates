<?xml version="1.0"?>
<Container version="2">
  <Name>SFTP</Name>
  <Repository>atmoz/sftp</Repository>
  <Registry>https://hub.docker.com/r/atmoz/sftp/</Registry>
  <Date>2016-09-04</Date>
  <Beta>True</Beta>
  <Project>https://github.com/atmoz/sftp</Project>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Overview>Easy to use SFTP server</Overview>
  <Description>Easy to use SFTP server</Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>2222</HostPort>
        <ContainerPort>22</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/sftp/users.conf</HostDir>
      <ContainerDir>/etc/sftp-users.conf</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <Environment/>
</Container>
